<?xml version="1.0" encoding="utf-8"?>

<document
  cnxml-version='0.7'
  xmlns='http://cnx.rice.edu/cnxml'
  xmlns:bib='http://bibtexml.sf.net/'
  xmlns:m='http://www.w3.org/1998/Math/MathML'
  xmlns:md='http://cnx.rice.edu/mdml/0.4'
  xmlns:md1='http://cnx.rice.edu/mdml'
  xmlns:q='http://cnx.rice.edu/qml/1.0'
>
  <title>Test Module</title>
  <content>
    <para>Audio and Video</para>
    <media
      alt='alternate'
      id='id123'
    >
      <audio
        autoplay='true'
        controller='true'
        loop='true'
        mime-type='audio/mp3'
        src='file.mp3'
        volume='0'
        standby='true'
      >
        <param
          name='author'
          value='John Doe'
        ></param>
      </audio>
      <video
        autoplay='true'
        controller='true'
        loop='true'
        mime-type='video/mp4'
        src='file.mp4'
        volume='11'
      >
        <param
          name='author'
          value='John Doe'
        ></param>
      </video>
      <audio
        for='pdf'
        mime-type='audio/mp3'
        src='file.mp3'
      ></audio>
      <audio
        for='pdf'
        mime-type='video/mp4'
        src='file.mp4'
      ></audio>
    </media>
    <para>Flash examples</para>
    <media
      alt='alternate'
      id='id123'
      longdesc='url.txt'
    >
      <flash
        mime-type='application/x-shockwave-flash'
        src='file.swf'
      ></flash>
      <flash
        height='100'
        id='id-flash'
        longdesc='url.txt'
        mime-type='application/x-shockwave-flash'
        src='file.swf'
        width='100'
        wmode='opaque'
      >
        <param
          name='flash-name'
          value='flash-value'
          xmlns='http://www.w3.org/1999/xhtml'
        ></param>
      </flash>
    </media>
    <para>Labview Examples</para>
    <media
      alt='alternate'
      id='id-labview'
    >
      <labview
        mime-type='application/x-labview-vi'
        pluginspage='http://digital.ni.com/express.nsf/bycode/exwgjq'
        src='file.vi'
        version='7.0'
        viname='the-path'
      ></labview>
      <labview
        for='online'
        height='100'
        id='id-labview-1'
        longdesc='url.txt'
        mime-type='application/x-labview-vi'
        pluginspage='http://digital.ni.com/express.nsf/bycode/exwgjq'
        src='file.vi'
        version='7.0'
        viname='the-path'
        width='200'
      ></labview>
    </media>
    <para>Java Examples</para>
    <media
      alt='alternate'
      id='id-java-applet'
    >
      <java-applet
        code='org.cnx.Applet.class'
        mime-type='application/x-java-applet'
        src=''
      ></java-applet>
      <java-applet
        archive='src.jar,more.jar'
        code='org.cnx.Applet.class'
        codebase='src/path'
        for='online'
        height='100'
        id='id-java-applet-1'
        longdesc='url.txt'
        mime-type='application/x-java-applet'
        name='Applet'
        src='Applet.java'
        width='200'
      >
        <param
          name='param-name'
          value='param-value'
          xmlns='http://www.w3.org/1999/xhtml'
        ></param>
      </java-applet>
    </media>
    <para>Hacky way of using Images to make a link to a jar file</para>
    <media
      alt='alternate'
    >
      <image
        src='equation-grapher_en.jar'
        thumbnail='PhET_Icon.png'
        width='450'
      ></image>
    </media>
    <para>But for PDF do not do anything special</para>
    <media
      alt='alternate'
    >
      <image
        for='pdf'
        mime-type='image/png'
        src='equation-grapher_en.jar'
        thumbnail='PhET_Icon.png'
        width='450'
      ></image>
    </media>
  </content>
</document>
